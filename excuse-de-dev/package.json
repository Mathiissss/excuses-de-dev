{
    "name": "excuses-de-dev",
    "version": "1.0.0",
    "description": "Générateur d'excuses de développeur",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server/index.js",
        "client": "cd client && npm start",
        "build": "cd client && npm run build",
        "install-all": "npm install && cd client && npm install"
    },
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.1"
    },
    "keywords": [
        "excuses",
        "dev",
        "generator"
    ],
    "author": "Mathis Barrois",
    "license": "MIT"
}
